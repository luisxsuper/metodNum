link<-"/meme.png"
main<-"Tu cara cuando ves"
sub<-"un meme hecho en R"
con <- url(link, open = 'rb')
rawpng <- readBin(con, what = 'raw', n = 50000)
close(con)
img <- png::readPNG(rawpng)
old_par <- par(no.readonly = TRUE)
on.exit(par(old_par))
par(xpd = NA, mgp = c(0, 0, 0), oma = c(0, 0, 0, 0), ann = F)
