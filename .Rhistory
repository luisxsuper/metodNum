plot.window(0:1, 0:1)
usr <- par("usr")
rasterImage(img, usr[1], usr[3], usr[2], usr[4])
text(.5, 1.05,  main, cex = 2, col = "#000000", pos = 3)
text(.5, -.05,   sub, cex = 2, col = "#000000", pos = 1)
